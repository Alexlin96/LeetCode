{
  "fileheader.customMade": {},
  "fileheader.cursorMode": {
    "title": "",
    "description": "",
    "keyword": "",
    "date": ""
  },
  "fileheader.configObj": {
    "autoAdd": false, // 检测文件没有头部注释，自动添加文件头部注释
    "language": {
      // js后缀文件
      "js": {
        "head": "/$$",
        "middle": " $ @",
        "end": " $/",
        // 函数自定义注释符号：如果有此配置 会默认使用
        "functionSymbol": {
          "head": "/*", // 统一增加几个*号
          "middle": " * @",
          "end": " */"
        }
      }
    }
  }
}
